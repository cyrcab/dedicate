{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["node_modules/", "assets/"],
  "rules": {
    "comma-dangle": 1,
    "object-curly-newline": 0,
    "import/newline-after-import": 1,
    "object-shorthand": 1,
    "no-param-reassign": 1,
    "consistent-return": 1,
    "default-param-last": 1,
    "import/prefer-default-export": 1,
    "import/no-unresolved": 1,
    "import/extensions": 0
  }
}
