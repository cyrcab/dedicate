{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["node_modules/", "BDD/", "prisma/"],
  "rules": {
    "comma-dangle": 1,
    "object-curly-newline": 0,
    "import/newline-after-import": 1,
    "object-shorthand": 1,
    "no-param-reassign": 1,
    "consistent-return": 1
  }
}
