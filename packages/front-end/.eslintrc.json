{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "comma-dangle": 1,
    "object-curly-newline": 1,
    "import/newline-after-import": 1,
    "object-shorthand": 1,
    "no-param-reassign": 1,
    "consistent-return": 1,
    "react/react-in-jsx-scope": 0
  }
}
